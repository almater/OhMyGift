<%@ page language="java" contentType="text/html; charset=UTF-8"    pageEncoding="UTF-8"%><%	//전달받은 파라미터들의 값들을 변수에 저장한다.	request.setCharacterEncoding("utf-8");	String name = request.getParameter("name");	String uid = request.getParameter("uid");	String img = request.getParameter("img");		session.setAttribute("myName", name);	session.setAttribute("myUid", uid);	session.setAttribute("myImg", img);%>