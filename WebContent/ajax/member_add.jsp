<%@ page language="java" contentType="text/html; charset=UTF-8"    pageEncoding="UTF-8"%><%@ page import="ohmygift.member.db.*, java.util.* " %><jsp:useBean id="member" class="ohmygift.member.db.MemberDAO"></jsp:useBean><%	//전달받은 파라미터들의 값들을 변수에 저장한다.	request.setCharacterEncoding("utf-8");	String name = request.getParameter("name");	String birthday = request.getParameter("birthday");	String uid = request.getParameter("uid");	String image = request.getParameter("image");	member.addMember(name,birthday,uid,image);%>